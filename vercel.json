{
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    }
  ],
  "headers": [
    {
      "source": "/(favicon.ico|icon.svg|favicon.svg|apple-icon.svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(favicon-.*\\.svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}